set nocompatible
set number
set tabstop=2
set shiftwidth=2
set expandtab
set autoindent
set smartindent
set ignorecase
set clipboard=unnamedplus
set mouse=a
set background=dark
set termguicolors
syntax on

let mapleader = " "
let g:netrw_keepdir = 0
let g:netrw_banner = 0

nnoremap - :e .<CR>
vnoremap < <gv
vnoremap > >gv

